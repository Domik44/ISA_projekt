{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "build",
            "command": "/usr/bin/gcc-9",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${workspaceFolder}/flow.c",
                "${workspaceFolder}/time.c",
                "${workspaceFolder}/list.c",
                "${workspaceFolder}/sender.c",
                "${workspaceFolder}/arguments.c",
                "${workspaceFolder}/flow.h",
                "-o",
                "${workspaceFolder}/../flow",
                "-lpcap"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}